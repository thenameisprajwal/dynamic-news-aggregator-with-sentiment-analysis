{
  "name": "ai-news-pulse-backend",
  "version": "1.0.0",
  "description": "Backend for AI News Pulse - Dynamic News Aggregator with Sentiment Analysis",
  "main": "src/server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "start": "node --import ./src/polyfills.js src/server.js",
    "dev": "node --import ./src/polyfills.js --watch src/server.js",
    "scrape": "node --import ./src/polyfills.js src/scripts/scrapeNews.js"
  },
  "keywords": [
    "news",
    "aggregator",
    "sentiment-analysis",
    "ai"
  ],
  "author": "",
